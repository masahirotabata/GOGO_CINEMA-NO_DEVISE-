 <%= form_with model: @movie, url:admin_movie_path(@movie), local: true, method: :patch do |f| %>
          <p>ユーザー名</p>
          <%= f.text_field :movie_name %>
          <p>画像</p>
          <%= f.attachment_field :image %>
          <p>映画あらすじ</p>
           <%= f.text_field :detail %>
          <%= f.submit "映画更新", class:"btn btn-success" %>

        <% end %>